{
    "functions": {
        "api/**/*.ts": {
            "includeFiles": "api/services/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/"
        }
    ]
}